WITH TEMP1 AS (
    SELECT ''    AS C1, 'T1'   AS C2, 'TEST1' AS C3 FROM DUAL UNION ALL
    SELECT 'T1'  AS C1, 'T11'  AS C2, 'TEST11' AS C3 FROM DUAL UNION ALL
    SELECT 'T1'  AS C1, 'T12'  AS C2, 'TEST12' AS C3 FROM DUAL UNION ALL
    SELECT 'T1'  AS C1, 'T13'  AS C2, 'TEST13' AS C3 FROM DUAL UNION ALL
    SELECT 'T13' AS C1, 'T131' AS C2, 'TEST131' AS C3 FROM DUAL UNION ALL
    SELECT 'T13' AS C1, 'T132' AS C2, 'TEST132' AS C3 FROM DUAL UNION ALL
    SELECT ''    AS C1, 'T2'   AS C2, 'TEST2' AS C3 FROM DUAL UNION ALL
    SELECT 'T2'  AS C1, 'T21'  AS C2, 'TEST21' AS C3 FROM DUAL UNION ALL
    SELECT 'T21' AS C1, 'T221' AS C2, 'TEST221' AS C3 FROM DUAL UNION ALL
    SELECT 'T21' AS C1, 'T222' AS C2, 'TEST222' AS C3 FROM DUAL UNION ALL
    SELECT 'T21' AS C1, 'T223' AS C2, 'TEST223' AS C3 FROM DUAL
)
SELECT TA.*,
       SYS_CONNECT_BY_PATH(C3, '//') AS CONNECT_CHAR,
       LEVEL,
       CONNECT_BY_ISLEAF
  FROM TEMP1 TA
 WHERE 1 = 1
 START WITH C1 IS NULL
 CONNECT BY PRIOR C2 = C1
 ORDER SIBLINGS BY C2 