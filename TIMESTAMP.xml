SELECT  *  
  FROM TB_USER  AS OF TIMESTAMP(SYSTIMESTAMP-INTERVAL '3' SECOND)  
 WHERE 1 = 1;
SELECT  *  
  FROM TB_USER  AS OF TIMESTAMP(SYSTIMESTAMP-INTERVAL '3' MINUTE)  
 WHERE 1 = 1;
SELECT  *  
  FROM TB_USER  AS OF TIMESTAMP(SYSTIMESTAMP-INTERVAL '3' HOUR)  
 WHERE 1 = 1; 
 
 
UPDATE TB_USER TA
   SET AGE = (SELECT AGE 
                FROM TB_USER  AS OF TIMESTAMP(SYSTIMESTAMP-INTERVAL '20' MINUTE)  
               WHERE 1 = 1
                 AND USER_ID = TA.USER_ID)
 WHERE 1 = 1 